{
  "name": "vscode-websearchparticipant",
  "displayName": "vscode-websearchparticipant",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.94.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "enabledApiProposals": [
      "lmTools"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "chatParticipants": [
        {
            "id": "vscode-websearchparticipant.tools",
            "fullName": "Tool User",
            "name": "tools",
            "description": "I use tools",
            "isSticky": true,
            "supportsToolReferences": true
        }
    ],
    "languageModelTools": [
        {
            "id": "vscode-websearchparticipant_tabCount",
            "tags": ["editors", "vscode-websearchparticipant"],
            "name": "tabCount",
            "displayName": "Tab Count",
            "modelDescription": "The number of active tabs in a tab group",
            "icon": "$(files)",
            "canBeInvokedManually": true,
            "parametersSchema": {
                "type": "object",
                "properties": {
                    "tabGroup": {
                        "type": "number",
                        "description": "The index of the tab group to check. This is optional- if not specified, the active tab group will be checked.",
                        "default": 0
                    }
                }
            },
            "supportedContentTypes": [
                "text/plain"
            ]
        },
        {
            "id": "vscode-websearchparticipant_findFiles",
            "tags": [
                "files",
                "search",
                "vscode-websearchparticipant"
            ],
            "displayName": "Find Files",
            "modelDescription": "Search for files in the current workspace",
            "parametersSchema": {
                "type": "object",
                "properties": {
                    "pattern": {
                        "type": "string",
                        "description": "Search for files that match this glob pattern"
                    }
                },
                "required": ["pattern"]
            },
            "supportedContentTypes": [
                "text/plain"
            ]
        },
        {
            "id": "vscode-websearchparticipant_runInTerminal",
            "tags": [
                "terminal",
                "vscode-websearchparticipant"
            ],
            "displayName": "Run in Terminal",
            "modelDescription": "Run a command in a terminal and return the output",
            "parametersSchema": {
                "type": "object",
                "properties": {
                    "command": {
                        "type": "string",
                        "description": "The command to run"
                    }
                },
                "required": [
                    "command"
                ]
            },
            "supportedContentTypes": [
                "text/plain"
            ],
            "requiresConfirmation": true
        }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.94.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.57.0",
    "esbuild": "^0.21.4",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  },
  "dependencies": {
    "@types/jsdom": "^21.1.6",
    "cheerio": "^1.0.0-rc.12",
    "jsdom": "^24.0.0"
  }
}
