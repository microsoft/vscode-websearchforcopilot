{
	"name": "vscode-websearchparticipant",
	"displayName": "vscode-websearchparticipant",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.94.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"enabledApiProposals": [
		"lmTools"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"authentication": [
			{
				"id": "tavily",
				"label": "Tavily"
			}
		],
		"commands": [
			{
				"command": "vscode-websearchparticipant.scrapeAndGetChunks",
				"title": "Scrape and Show Related Chunks Ranked by TF-IDF"
			},
			{
				"command": "vscode-websearchparticipant.scrapeAndGetNaiveChunks",
				"title": "Scrape and Show Related Naive Chunks Ranked by TF-IDF"
			},
			{
				"command": "vscode-websearchparticipant.scrapeAndGetNaiveChunksFromEmbeddings",
				"title": "Scrape and Show Related Naive Chunks Ranked by Embeddings"
			},
			{
				"command": "vscode-websearchparticipant.tavilyScrapeAndGetChunks",
				"title": "Scrape and Show Related Tavily Chunks"
			},
			{
				"command": "vscode-websearchparticipant.scrapeAndOpenWholeDocument",
				"title": "Scrape and Open Info As Document"
			},
			{
				"command": "vscode-websearchparticipant.tavilyScrapeAndOpenWholeDocument",
				"title": "Tavily Extract and Open Info As Document"
			},
			{
				"title": "Hi",
				"command": "tavily-authentication-provider.hi",
				"category": "Tavily"
			},
			{
				"title": "Tavily Search",
				"command": "vscode-websearchparticipant.tavilySearch",
				"category": "Web Search"
			},
			{
				"title": "Bing Search",
				"command": "vscode-websearchparticipant.bingSearch",
				"category": "Web Search"
			}
		],
		"chatParticipants": [
			{
				"id": "vscode-websearchparticipant.websearch",
				"fullName": "Web Search",
				"name": "websearch",
				"description": "Search the web for relevant up-to-date information",
				"isSticky": true,
				"supportsToolReferences": true
			}
		],
		"languageModelTools": [
			{
				"id": "vscode-websearchparticipant_chunkedWebContent",
				"tags": [
					"web",
					"search",
					"vscode-websearchparticipant"
				],
				"displayName": "Chunked Web Content",
				"canBeInvokedManually": true,
				"name": "chunkedWebContent",
				"icon": "search",
				"userDescription": "Find relevant content on webpages",
				"modelDescription": "From a list of webpages, find relevant content based on a query",
				"parametersSchema": {
					"type": "object",
					"properties": {
						"query": {
							"type": "string",
							"description": "Search for content on these websites based on this query"
						},
						"urls": {
							"type": "string",
							"description": "The URLs to search for content on, separated by commas"
						}
					},
					"required": [
						"query",
						"urls"
					]
				},
				"supportedContentTypes": [
					"text/plain"
				]
			},
			{
				"id": "vscode-websearchparticipant_publicWebSearch",
				"tags": [
					"web",
					"search",
					"vscode-websearchparticipant"
				],
				"displayName": "Web Search",
				"canBeInvokedManually": true,
				"name": "websearch",
				"icon": "globe",
				"userDescription": "Search the web for relevant up-to-date information",
				"modelDescription": "Search the web for relevant up-to-date information",
				"parametersSchema": {
					"type": "object",
					"properties": {
						"query": {
							"type": "string",
							"description": "The query to search the web for"
						}
					},
					"required": [
						"query"
					]
				},
				"supportedContentTypes": [
					"text/plain"
				]
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir dist",
		"watch-tests": "tsc -p . -w --outDir dist",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "vscode-test"
	},
	"devDependencies": {
		"@types/mocha": "^10.0.6",
		"@types/node": "18.x",
		"@types/vscode": "^1.86.0",
		"@typescript-eslint/eslint-plugin": "^6.13.1",
		"@typescript-eslint/parser": "^6.13.1",
		"@vscode/test-cli": "^0.0.4",
		"@vscode/test-electron": "^2.3.8",
		"eslint": "^8.54.0",
		"ts-loader": "^9.5.1",
		"typescript": "^5.3.2",
		"webpack": "^5.91.0",
		"webpack-cli": "^5.1.4"
	},
	"dependencies": {
		"@azure-rest/ai-inference": "latest",
		"@azure/core-auth": "latest",
		"@azure/core-sse": "latest",
		"@microsoft/tiktokenizer": "^1.0.8",
		"@types/jsdom": "^21.1.6",
		"cheerio": "^1.0.0-rc.12",
		"jsdom": "^24.0.0"
	}
}
